{
  "name": "decision-maker",
  "version": "1.0.0",
  "description": "Http triggered lambda functions for decision making, which can be integrated with slack commands.",
  "scripts": {
    "deploy": "./node_modules/serverless/bin/serverless deploy -v",
    "lint": "eslint src --ext .js,.ts",
    "release": "semantic-release --debug",
    "remove": "./node_modules/serverless/bin/serverless remove -v",
    "start": "serverless offline start",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged",
      "commit-msg": "echo $HUSKY_GIT_PARAMS && commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": "eslint src --ext .js,.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaypeng2015/decision-maker.git"
  },
  "keywords": [
    "decision",
    "roll",
    "dice",
    "coin",
    "Step Functions"
  ],
  "engines": {
    "node": ">= 8.10.0"
  },
  "author": "Jingwei Peng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaypeng2015/decision-maker/issues"
  },
  "homepage": "https://github.com/jaypeng2015/decision-maker#readme",
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@types/aws-lambda": "8.10.46",
    "@types/jest": "25.1.4",
    "@types/lodash": "4.14.149",
    "@types/node": "10.12.24",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/eslint-plugin-tslint": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "lint-staged": "10.0.9",
    "prettier": "1.19.1",
    "semantic-release": "17.0.4",
    "serverless": "1.67.0",
    "serverless-offline": "5.12.1",
    "serverless-pseudo-parameters": "2.5.0",
    "serverless-step-functions": "2.17.2",
    "serverless-step-functions-offline": "2.1.4",
    "serverless-webpack": "5.3.1",
    "source-map-support": "0.5.16",
    "timekeeper": "2.2.0",
    "ts-jest": "25.2.1",
    "ts-loader": "6.2.2",
    "typescript": "3.8.3",
    "webpack": "4.42.1",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "aws-sdk": "2.536.0",
    "aws-xray-sdk": "2.5.0",
    "axios": "0.19.2",
    "qs": "6.9.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": ".*test\\.(ts|tsx|js)$"
  }
}
