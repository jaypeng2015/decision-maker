{
  "name": "decision-maker",
  "version": "1.0.0",
  "description": "Http triggered lambda functions for decision making, which can be integrated with slack commands.",
  "main": "index.js",
  "scripts": {
    "test": "scripts/test",
    "pre-commit": "scripts/pre-commit",
    "lint": "scripts/lint",
    "deploy": "./node_modules/serverless/bin/serverless deploy -v",
    "remove": "./node_modules/serverless/bin/serverless remove -v",
    "release": "semantic-release --debug",
    "start": "serverless offline start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "echo $HUSKY_GIT_PARAMS && commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": "eslint -c .eslintrc ./src --ext .js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaypeng2015/decision-maker.git"
  },
  "keywords": [
    "decision",
    "roll",
    "dice",
    "coin",
    "Step Functions"
  ],
  "engines": {
    "node": ">= 8.10.0"
  },
  "author": "Jingwei Peng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaypeng2015/decision-maker/issues"
  },
  "homepage": "https://github.com/jaypeng2015/decision-maker#readme",
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@babel/plugin-proposal-class-properties": "7.2.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/preset-env": "7.2.0",
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@semantic-release/exec": "3.3.0",
    "@semantic-release/git": "7.0.5",
    "babel-loader": "8.0.4",
    "eslint": "5.9.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-no-loops": "0.3.0",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-prettier": "3.0.0",
    "husky": "1.2.0",
    "lint-staged": "8.1.0",
    "prettier": "1.15.3",
    "semantic-release": "15.12.3",
    "serverless": "1.34.1",
    "serverless-offline": "3.31.3",
    "serverless-plugin-tracing": "2.0.0",
    "serverless-prune-plugin": "1.3.2",
    "serverless-pseudo-parameters": "2.2.0",
    "serverless-step-functions": "1.8.0",
    "serverless-step-functions-offline": "2.1.2",
    "serverless-webpack": "5.2.0",
    "timekeeper": "2.1.2",
    "webpack": "4.26.1"
  },
  "dependencies": {
    "aws-sdk": "2.290.0",
    "aws-xray-sdk": "2.1.0",
    "qs": "6.6.0",
    "request": "2.88.0"
  }
}
